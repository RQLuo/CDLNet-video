{
    "type": "CDLNetVideo",
    "model": {
        "adaptive": true,
        "K": 30,
        "M": 64,
        "C": 1,
        "P": 7,
        "s": 1,
        "t0": 0,
        "init": true
    },
    "paths": {
        "save": "trained_nets/CDLNetVideo-syns1",
        "ckpt": "trained_nets/CDLNetVideo-syns1/net_epoch_340.ckpt"
    },
    "train": {
        "loaders": {
            "trn_path_list": ["data_gen/davis/train"],
            "val_path_list": ["data_gen/davis/val"],
            "tst_path_list": ["data_gen/davis/test"],
            "crop_size": 128,
            "batch_size": [1, 1, 1],
            "load_color": false,
            "depth": 16
        },
        "opt": {
            "lr": 5e-5
        },
        "sched": {
            "step_size": 10,
            "gamma": 0.9
        },
        "fit": {
            "epochs": 5000,
            "noise_std": 25,
            "backtrack_thresh": 2,
            "demosaic": false,
            "verbose": true,
            "val_freq": 50,
            "save_freq": 10,
            "mcsure": false
        }
    }
}
