{
    "type": "CDLNetVideo",
    "model": {
        "adaptive": true,
        "K": 30,
        "M": 169,
        "C": 1,
        "P": 7,
        "s": 2,
        "t0": 0,
        "init": true
    },
    "paths": {
        "save": "trained_nets/CDLNetVideo",
        "ckpt": null
    },
    "train": {
        "loaders": {
            "trn_path_list": ["data_gen/davis/train"],
            "val_path_list": ["data_gen/davis/val"],
            "tst_path_list": ["data_gen/davis/test"],
            "crop_size": 128,
            "batch_size": [2, 1, 1],
            "load_color": false,
            "depth": 16,
            "crop_ratio": 0.95,   
            "aug_prob": 0.9,    
            "max_shift": 25
        },
        "opt": {
            "lr": 7e-5
        },
        "sched": {
            "step_size": 20,
            "gamma": 0.95
        },
        "fit": {
            "epochs": 5000,
            "noise_std": [20,30],
            "backtrack_thresh": 2,
            "demosaic": false,
            "verbose": true,
            "val_freq": 50,
            "save_freq": 10,
            "mcsure": false
        }
    }
}
