{
"type": "DnCNN",
"model": {
	"K": 9,
	"M": 32,
	"Ci": 1,
	"Co": 1
},
"paths": {
	"save": "nets/test",
	"ckpt": null
},
"train": {
	"loaders": {
		"batch_size": 1,
		"crop_size": 64,
		"trn_path_list": ["dataset/CBSD432"],
		"val_path_list": ["dataset/Kodak"],
		"tst_path_list": ["dataset/CBSD68"],
		"load_color": false
	},
	"fit": {
		"epochs": 20,
		"noise_std": 25,
		"val_freq": 5,
		"save_freq": 5,
		"backtrack_thresh": 2,
		"verbose": true,
		"clip_grad": 5e-2,
		"demosaic": false
	},
	"opt": {
		"lr": 1e-3
	},
	"sched": {
		"gamma": 0.95,
		"step_size": 50
	}
}
}
